{
  "name": "portfolio-project-frontend",
  "version": "1.0.0",
  "description": "all frontend stack is here!",
  "main": "index.js",
  "repository": "https://github.com/DocDuck/portfolio-project.git",
  "author": "Aleksandr Mitin <alex.mitin.psmu@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --env port=3000 --open",
    "lint:ts": "eslint src/**/*.{ts,tsx}",
    "lint:ts:fix": "eslint src/**/*.{ts,tsx} --fix",
    "lint:scss": "stylelint **/*.scss",
    "lint:scss:fix": "stylelint **/*.scss --fix",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "test:jest": "jest --config ./config/jest/jest.config.ts",
    "test:loki": "loki test --config ./config/storybook",
    "test:loki:update": "loki update --config ./config/storybook",
    "test:loki:approve": "loki approve --diffOnly --config ./config/storybook",
    "test:loki:ci": "loki --requireReference --reactUri file:$PWD/storybook-static",
    "test:loki:json": "node scripts/generateLokiReport.mjs",
    "test:loki:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:loki:report": "npm run test:loki:json && npm run test:loki:html",
    "storybook:run": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@chromatic-com/storybook": "^1.8.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/addon-themes": "^8.2.9",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.3",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-webpack5": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/webpack-dev-server": "^4.7.2",
    "babel-jest": "^29.7.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transformer-svg": "^2.0.2",
    "loki": "^0.35.1",
    "mini-css-extract-plugin": "^2.8.1",
    "react-test-renderer": "^18.3.1",
    "reg-cli": "^0.18.6",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "storybook": "^8.2.9",
    "style-loader": "^4.0.0",
    "stylelint": "^16.7.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "webpack": "^5.93.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "classnames": "^2.5.1",
    "i18next": "^23.11.2",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.22.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
